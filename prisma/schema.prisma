generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Session {
  id          String   @id @default(cuid())
  shop        String   @unique
  accessToken String
   scope       String   @db.Text
  expires     DateTime?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
